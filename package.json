{
    "name": "markdown-toc",
    "displayName": "Markdown-Toc",
    "description": "",
    "version": "0.0.1",
    "publisher": "AlanWalk",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.createMarkdownToc",
        "onCommand:extension.updateMarkdownToc",
        "onCommand:extension.deleteMarkdownToc"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.createMarkdownToc",
            "title": "Markdown Toc: Create"
        },
        {
            "command": "extension.updateMarkdownToc",
            "title": "Markdown Toc: Update"
        },
        {
            "command": "extension.deleteMarkdownToc",
            "title": "Markdown Toc: Delete"
        }]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}